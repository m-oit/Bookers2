<h1>画像投稿フォーム</h1>

<% if @book.errors.any? %>
 <%= @book.errors.count %>errors prohibited this obj from being saved:
 <ul>
     <% @book.errors.full_messages.each do |message| %>
     <li><%= message %></li>
     <% end %>
 </ul>
 <% end %>

<%= form_with model: @book do |f| %>

<h4>画像</h4>
<%= f.file_field :image, accept: "image/*" %>
<h4>title</h4>
<%= f.text_field :title %>
<h4>body</h4>
<%= f.text_area :body %>
<%= f.submit '投稿' %>
<% end %>